{
  "name": "io-hbase",
  "title": "HBase REST API Client for Node",
  "keywords": [
    "hbase",
    "hbase rest"
  ],
  "license": "MIT",
  "version": "0.0.6",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.12.0"
  },
  "main": "./dist/index.js",
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "gitbook-cli": "^2.3.2",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tsutils": "^3.2.0",
    "typescript": "^3.1.3",
    "ts-jest": "^24.0.2"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp dist",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.release.json && cp src/types.d.ts dist/types.d.ts && printf \"import './types';\n$(cat ./dist/index.d.ts)\" > ./dist/index.d.ts",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.release.json\"",
    "pretest": "npm run lint && tsc -p tsconfig.json",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "gitbookBuild": "cd docs && rimraf docs && gitbook build && mv ./_book ./docs",
    "gitbookServe": "cd docs && gitbook serve"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mosfet1kg/io-hbase.git"
  }
}
